CREATE TABLE IF NOT EXISTS "products" (
  "id" uuid NOT NULL,
  "name" text NOT NULL,
  "tags" text NOT NULL,
  "categories" text,
  "metadata" jsonb DEFAULT {},
  "prices" text NOT NULL,
  PRIMARY KEY ("id")
);
COMMENT ON TABLE "products" IS 'uid: tbl:Product';
COMMENT ON COLUMN "products"."id" IS 'uid: col_products_id';
COMMENT ON COLUMN "products"."name" IS 'uid: col_products_name';
COMMENT ON COLUMN "products"."tags" IS 'uid: col_products_tags';
COMMENT ON COLUMN "products"."categories" IS 'uid: col_products_categories';
COMMENT ON COLUMN "products"."metadata" IS 'uid: col_products_metadata';
COMMENT ON COLUMN "products"."prices" IS 'uid: col_products_prices';
