CREATE TABLE IF NOT EXISTS "products" (
  "id" uuid NOT NULL,
  "name" text NOT NULL,
  "tags" text NOT NULL,
  "categories" text,
  "metadata" jsonb DEFAULT {},
  "prices" text NOT NULL,
  PRIMARY KEY ("id")
);
COMMENT ON TABLE "products" IS 'uid: tbl:Product';
COMMENT ON COLUMN "products"."id" IS 'uid: col:Product.id';
COMMENT ON COLUMN "products"."name" IS 'uid: col:Product.name';
COMMENT ON COLUMN "products"."tags" IS 'uid: col:Product.tags';
COMMENT ON COLUMN "products"."categories" IS 'uid: col:Product.categories';
COMMENT ON COLUMN "products"."metadata" IS 'uid: col:Product.metadata';
COMMENT ON COLUMN "products"."prices" IS 'uid: col:Product.prices';
