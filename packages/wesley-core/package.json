{
  "name": "@wesley/core",
  "version": "0.1.0",
  "type": "module",
  "description": "Wesley core domain logic - pure functions, no dependencies",
  "main": "./src/index.mjs",
  "exports": {
    ".": "./src/index.mjs",
    "./domain": "./src/domain/index.mjs",
    "./application": "./src/application/index.mjs",
    "./ports": "./src/ports/index.mjs"
  },
  "files": [
    "src/"
  ],
  "scripts": {
    "test": "node --test test/**/*.test.mjs",
    "test:watch": "node --test --watch test/**/*.test.mjs",
    "test:coverage": "node --test --experimental-test-coverage test/**/*.test.mjs",
    "lint": "eslint src/"
  },
  "engines": {
    "node": ">=18.17"
  },
  "keywords": [
    "graphql",
    "schema",
    "domain",
    "hexagonal"
  ],
  "author": "Captain James",
  "license": "MIT",
  "dependencies": {
    "@supabase/pg-parser": "^0.1.3",
    "graphql": "^16.11.0"
  }
